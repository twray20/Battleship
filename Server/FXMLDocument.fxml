<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="395.0" prefWidth="533.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="salvoserver.FXMLDocumentController">
   <children>
      <Button fx:id="confirmShoot" layoutX="311.0" layoutY="160.0" mnemonicParsing="false" onAction="#shoot" text="Shoot Target" />
      <TextField fx:id="yShoot" alignment="CENTER" layoutX="203.0" layoutY="160.0" prefHeight="26.0" prefWidth="75.0" promptText="#1-10" />
      <TextField alignment="CENTER" layoutX="125.0" layoutY="160.0" prefHeight="26.0" prefWidth="75.0" promptText="L: A-J" fx:id="xShoot" />
      <Rectangle fx:id="messageBG" arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="60.0" layoutX="115.0" layoutY="249.0" stroke="BLACK" strokeType="INSIDE" width="304.0" />
      <Label layoutX="240.0" layoutY="14.0" text="Player 1" underline="true" />
      <Label fx:id="playState" alignment="CENTER" layoutX="162.0" layoutY="44.0" prefHeight="16.0" prefWidth="211.0" text="Setup" textAlignment="CENTER" />
      <TextField layoutX="204.0" layoutY="160.0" prefHeight="26.0" prefWidth="63.0" promptText="L: A-J" fx:id="xCoord" />
      <TextField fx:id="yCoord" layoutX="274.0" layoutY="160.0" prefHeight="26.0" prefWidth="63.0" promptText="#1-10" />
      <Label fx:id="startPoint" layoutX="225.0" layoutY="141.0" text="Starting point" />
      <Label fx:id="shipSetup" layoutX="76.0" layoutY="165.0" text="5 Space Battleship" />
      <Label fx:id="endPoint" layoutX="229.0" layoutY="208.0" text="Ending Point" />
      <Button fx:id="startConfirm" layoutX="345.0" layoutY="160.0" mnemonicParsing="false" onAction="#confirm" text="Confirm Start" />
      <Button fx:id="choice1" layoutX="117.0" layoutY="236.0" mnemonicParsing="false" onAction="#nextShip1" text="Choice 1" />
      <Button fx:id="choice2" layoutX="192.0" layoutY="236.0" mnemonicParsing="false" onAction="#nextShip2" text="Choice 2" />
      <Button fx:id="choice3" layoutX="268.0" layoutY="236.0" mnemonicParsing="false" onAction="#nextShip3" text="Choice 3" />
      <Button fx:id="choice4" layoutX="345.0" layoutY="236.0" mnemonicParsing="false" onAction="#nextShip4" text="Choice 4" />
      <Label fx:id="message" alignment="CENTER" layoutX="117.0" layoutY="271.0" prefHeight="16.0" prefWidth="298.0" text="Message Board" textAlignment="CENTER" textFill="#fffafa">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Label>
      <Label fx:id="turn" alignment="CENTER" layoutX="229.0" layoutY="69.0" text="Player Turn" textAlignment="CENTER" />
   </children>
</AnchorPane>
